<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.github.antonocean.thebangandroid">

<uses-permission android:name="android.permission.INTERNET" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.github.antonocean.thebangandroid.activity.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:launchMode="singleTop"
        android:name="com.github.antonocean.thebangandroid.activity.MainActivity2_search"
        android:parentActivityName="com.github.antonocean.thebangandroid.activity.MainActivity" />
    <activity android:launchMode="singleTop"
        android:name="com.github.antonocean.thebangandroid.activity.MainActivity3_detail"
        android:label="@string/title_activity_main3"
        android:parentActivityName="com.github.antonocean.thebangandroid.activity.MainActivity2_search" />
    <activity android:launchMode="singleTop"
        android:name="com.github.antonocean.thebangandroid.activity.MainActivity4_cart"
        android:parentActivityName="com.github.antonocean.thebangandroid.activity.MainActivity" />
    <activity android:launchMode="singleTop" android:name="com.github.antonocean.thebangandroid.activity.MainActivity5_about_me"
        android:parentActivityName="com.github.antonocean.thebangandroid.activity.MainActivity" ></activity>
</application>

</manifest>